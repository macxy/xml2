<script>
	function fnCol_1(){
		$.ajax({
			type: "GET",
			url: "result.xml",
			dataType: "xml",
			success: function(xml){
		$(xml).find("result").each(function() {
			var number = $(this).find("number").text();
			var silks = $(this).find("col_1").text();
			var horse = $(this).find("horse").text();
			var three_place = $(this).find("three_place").text();
			var betting_with = $(this).find("betting_with").text();
			var previous_win_odd = $(this).find("previous_win_odd").text();
			var win_or_ew = $(this).find("win_or_ew").text();


			var output = "<table border=1><tr>";
			output	+= '<td width=50 align=center>' + number + '</td>';
			output	+= '<td width=50 align=center>' + silks + '</td>';
			output	+= '<td width=50 align=center>' + horse + '</td>';
			output	+= '<td width=50 align=center>' + three_place + '</td>';
			output	+= '<td width=50 align=center>' + betting_with + '</td>';
			output	+= '<td width=50 align=center>' + previous_win_odd + '</td>';
			output	+= '<td width=50 align=center>' + win_or_ew + '</td>';
			output += '</tr></table>';
			$("#demo").append(output);			
		});
	}
});;
	
}
</script>
<style>
	#HorseRacing{
		background: transparent;
